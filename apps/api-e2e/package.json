{
  "name": "@servir/api-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "tags": [
      "scope:api",
      "type:e2e"
    ],
    "implicitDependencies": [
      "@servir/api"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/api-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@servir/api:build",
          "@servir/api:serve"
        ]
      }
    }
  },
  "dependencies": {
    "@servir/contracts": "workspace:*",
    "axios": "^1.6.0"
  }
}
